    <p>Anna Beatriz</p>
         </div>

        <div id="toggles">
            <p>COZINHA</p>
            <input type="checkbox" name="checkbox1" id="checkbox1" class="ios-toggle" checked/>
            <label for="checkbox1" class="checkbox-label" data-off="" data-on="BOTAO DESLIGADO" ></label></label>
         </div>

        <div id="toggles">
             <p>QUARTO</p>
            <input type="checkbox" name="checkbox2" id="checkbox2" class="ios-toggle" checked/>
            <label for="checkbox2" class="checkbox-label" data-off="?acao=gpio5Off" data-on="|?acao=gpio5On" ></label></label>
         </div>
         <div id="toggles">
            <p>SALA</p>
           <input type="checkbox" name="checkbox2" id="checkbox3" class="ios-toggle" checked/>
           <label for="checkbox3" class="checkbox-label" data-off="?acao=gpio5Off" data-on="|?acao=gpio5On" ></label></label>
        
         
          </div>


         <div class="box">
            <div class="box-circle">
                <p>Consumo em Ampér</p>
                <svg>
                    <circle cx="70" cy="70" r="70"></circle>
                    <circle id="circleProgress" cx="70" cy="70" r="70"></circle>
                </svg></div>

</body>
</html>